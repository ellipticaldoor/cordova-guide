<?xml version='1.0' encoding='utf-8'?>
<widget id="com.guide.guide" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
	<name>Vsit</name>
	<description>
		Vsit.
	</description>
	<author email="dortamiguel@gmail.com" href="https://github.com/ellipticaldoor/">
		Vsit
	</author>
	<content src="index.html" />
	<plugin name="cordova-plugin-whitelist" version="1" />
	<access origin="*" />
	<allow-intent href="http://*/*" />
	<allow-intent href="https://*/*" />
	<allow-intent href="tel:*" />
	<allow-intent href="sms:*" />
	<allow-intent href="mailto:*" />
	<allow-intent href="geo:*" />
	<preference name="orientation" value="portrait" />
	<preference name="SplashScreen" value="screen"/>
	<preference name="SplashScreenDelay" value="2000" />
	<preference name="SplashMaintainAspectRatio" value="true" />
	<platform name="android">
		<allow-intent href="market:*" />
		<preference name="android-minSdkVersion" value="14" />
		<preference name="android-targetSdkVersion" value="23" />
		<!--
			<config-file target="AndroidManifest.xml" parent="/*">
				<uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS" />
				<uses-permission android:name="android.permission.RECORD_AUDIO" />
				<uses-permission android:name="android.permission.CAPTURE_AUDIO_OUTPUT" />
		</config-file>
		-->
	</platform>
	<platform name="ios">
		<allow-intent href="itms:*" />
		<allow-intent href="itms-apps:*" />
		<!-- <hook src="hooks/iosrtc-swift-support.js" type="after_platform_add" /> -->
	</platform>
</widget>
